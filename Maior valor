// Online C compiler to run C program online
#include <stdio.h>

int main() 
{
    int numeros(int N1, int N2, int N3, int nmaior);
    int N1, N2, N3, nmaior;
    
//atribuindo o primeira número como o maior
    printf("Primeiro número: ");
    scanf("%d", &N1);
    nmaior = N1;
    
    printf("Segundo número: ");
    scanf("%d", &N2);
    printf("Terceiro número: ");
    scanf("%d", &N3);
    
    numeros(N1, N2, N3, nmaior);
}

//função para calcular a média
int numeros(int N3, int N4, int N5, int maior)
{
//comparando os números para saber qual é a maior
    	if (N4 > maior)
  		    maior=N4;
  		else if(N5 > maior)
  		    maior=N5;
  		    
  	printf("O maior número é: %d",maior);
  	printf("\n");
    
    return;
}
