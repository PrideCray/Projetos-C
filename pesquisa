// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int vet[10], i=0, num;
    char achou ='f';
    
    for(i=0; i<=9; i++)
    {
        printf("Digite o vetor: ");
        scanf("%d", &vet[i]);
    }
    
    printf("Procure por um número: ");
    scanf("%d", &num);
    
    i=0;
    while(i<=9)
    {
        if(vet[i]==num)
        {
            achou='v';
            printf("O número está na posição: %d\n", i);
        }
        i++;
    }
    
    if(achou='f')
    {
        scanf("Número não encontrado");
    }
    
    
    
    return 0;
}
