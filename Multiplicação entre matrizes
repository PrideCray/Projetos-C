#include <stdio.h>

int main() {
    //declarando a matriz
    int m1[3][3];
    int m2[3][3];
    int result[3][3];
    int i, j, n;
    
    
    //variar as linhas
    for(i=0; i<=2; i++)
    {
        //variar as colunas
        for(j=0; j<=2;j++)
        {
            printf("Digite um nÃºmero na linha %d e coluna %d da primeira matriz\n", i, j);
            scanf("%d", &m1[i][j]);
        }
    }
    
    printf("\n");
    
    for(i=0; i<=2; i++)
    {
        //variar as colunas
        for(j=0; j<=2;j++)
        {
            printf("Digite um nÃºmero na linha %d e coluna %d da segunda matriz\n ", i, j);
            scanf("%d", &m2[i][j]);
        }
    }

    //multiplicar a matriz
    for(i=0; i<=2; i++)
    {
        for(j=0; j<=2;j++)
        {
            for(n=0; n<=2; n++)
            {
                result[i][j]=m1[i][j]*m2[i][j];
                printf("%d  ", result[i][j]);
            }
        }
        printf("\n");
    }
    
    return 0;
}
