#include <stdio.h>

int main() {
    //declarando a matriz
    int m[3][3];
    int i, j;
    
    //variar as linhas
    for(i=0; i<=2; i++)
    {
        //variar as colunas
        for(j=0; j<=2;j++)
        {
            printf("Digite um nÃºmero na linha %d e coluna %d\n", i, j);
            scanf("%d", &m[i][j]);
        }
    }

    //imprimir a matriz
    for(i=0; i<=2; i++)
    {
        for(j=0; j<=2;j++)
        {
            printf("%d  ", m[i][j]);
        }
        printf("\n");
    }
    
    return 0;
}
